{
  "name": "Carbon Syntax Highlighter",
  "icon": "./assets/icon.png",
  "displayName": "Carbon Syntax Highlighter",
  "description": "A syntax highlighter for the Carbon programming language",
  "version": "0.0.1",
  "author": "Rohan Vashisht",
  "publisher": "RohanVashisht",
  "repository": "https://github.com/RohanVashisht1234/sublime_text_carbon_syntax_highlighter",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "carbon",
      "aliases": ["carbon", "carbon"],
      "extensions": [".carbon"],
      "configuration": "./src/language-configuration.json"
    }],
    "grammars": [{
      "language": "carbon",
      "scopeName": "source.carbon",
      "path": "./src/carbon.tmLanguage.json"
    }]
  }
}
